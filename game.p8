function _init()
	player.h=3
	player.s=1
	
end

function _update60()
	if btn(⬅️) do xspd=-2 end
	if btn(➡️) do xspd=2 end
	if btn(⬆️) do yspd=-2 end
	if btn(⬇️) do yspd=2 end
	
	player.p.x+=xspd
	player.p.y+=yspd
	xspd=0
	yspd=0
end

function _draw()
	cls()
	p=player.p
	spr(player.s,p.x,p.y)
	
end
