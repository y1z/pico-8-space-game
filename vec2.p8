-- vector math
vec2 = {
	x=0,
	y=0,

-- make new vector with default values
	new=function(self,tbl)
	tbl=tbl or {}
	setmetatable(tbl,{
		__index=self
		})
	return tbl
	end,
	
	dot=function(self,other)
		return (self.x * other.x)+(self.y*other.y)
	end,
	
	print=function(self)
	print(tostr(self.x)..","..tostr(self.y))
	end,
	
	add=function(self,o)
	 nx=self.x+o.x
	 ny=self.y+o.y
	return self:new({x=nx,y=ny})
	end,
	
	madd=function(self,o)
	self.x=self.x+o.x
	self.y=self.y+o.y
	end, 
}
